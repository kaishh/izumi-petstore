{
  "compilerOptions": {
    "outDir": "../../../target/tsc_output",
    "module": "commonjs",
    "target": "es5",
    "lib": ["es6", "dom"],
    "sourceMap": true,
    "allowJs": false,
    "moduleResolution": "node",
    "rootDirs": [
      ".",
      "../../../../../api/petstore-api/target/scala-2.12/src_managed/main/"
    ],
    "baseUrl": ".",
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": false,
    "suppressImplicitAnyIndexErrors": true,
    "experimentalDecorators": true
  },
  "compileOnSave": false
}
